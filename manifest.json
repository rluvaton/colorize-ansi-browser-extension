{
  "name": "Colorize ANSI",
  "manifest_version": 3,
  "action": {},
  "version": "0.2.0",
  "icons": {
    "128": "images/icons/icon-128.png"
  },
  "description": "Replace <pre> tags that contains ansi colors with colored <pre> tags",
